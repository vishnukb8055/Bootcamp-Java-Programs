import java.util.HashSet;
import java.util.Scanner;

public class ParticipantIDFinder {
    public static void main(String[] args) {
        
        Scanner scanner = new Scanner(System.in);
        
        System.out.println("Enter the number of participants:");
        int n = scanner.nextInt();
        
        HashSet<Integer> participantIDs = new HashSet<>();
        
        System.out.println("Enter the IDs:");
        for (int i = 0; i < n; i++) {
            int id = scanner.nextInt();
            participantIDs.add(id);
        }
        
        while (true) {
            System.out.println("Enter ID to search:");
            int searchID = scanner.nextInt();
            
            if (searchID == 0) {
                System.out.println("Program terminated");
                break;
            }
            
            if (participantIDs.contains(searchID)) {
                System.out.println("Participant with ID " + searchID + " exists");
            } else {
                System.out.println("Participant with ID " + searchID + " does not exist");
            }
        }
        
        scanner.close();
    }
}
